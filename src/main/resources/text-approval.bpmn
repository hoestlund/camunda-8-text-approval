    </bpmn:endEvent>
    <bpmn:endEvent id="Event_00hj8kn" name="Text sent">
      <bpmn:incoming>Flow_1fnbycl</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_1klu1e7" sourceRef="StartEvent_1" targetRef="t_checkBadWords" />
